                                    halign: "left"
                                    font_style: "H6"
                                    #md_bg_color: "red"

                                MDTextField:

                                    id: ButtonMenuCashierPaymentType

                                    size_hint_x: 0.4
                                    text: "Dólar"
                                    icon_right: "arrow-down"

                                    mode: "rectangle"
                                    readonly: True 
                                    focus: True

                                    on_focus: if self.focus: root.CallbackMenuCashierPaymentType(self)

                            MDGridLayout:
                                cols: 3
                                spacing: dp(10)
