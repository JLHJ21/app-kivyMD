                                    hint_text: "Cédula del cliente"

                                    required: True

                                    helper_text_mode: "on_error"
                                    helper_text: "Ingrese una cédula." if len(self.text) == 0 else ""

                                    max_text_length: 10
                                    mode: "rectangle"
                                    line_color_normal: "red" if len(self.text) == 0 else "blue"

                            MDGridLayout:
