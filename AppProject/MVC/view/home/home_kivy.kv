                            MDGridLayout:
                                cols: 4
                                #padding: dp(1), dp(1), dp(1), dp(16)


                                MDLabel:
                                    size_hint_x: 0.025


                                MDIcon:
                                    padding: dp(1), dp(1), dp(1), dp(1)
                                    icon: "cash"
                                    font_size: '36sp' 
                                    size_hint_x: 0.4


                                MDLabel:
                                    size_hint_x: 0.05
                                    

                                
                                MDTextField:
                                    text: "100$"
                                    hint_text: "Dinero en dólares:"
                                    mode: "rectangle"
                                    disabled: True 
                                    focus: True
                                    size_hint_x: 0.4

                            MDGridLayout:
                                cols: 4
