<InitialPage>:
    name: "InitialPage"


    
    MDBoxLayout:
        id: boxLayoutAllScreen
        orientation: 'vertical'
        padding: dp(16), dp(root.manager.parent.parent.ids.toolbar.height), dp(16), dp(root.manager.parent.parent.ids.toolbarbottom.height)

        MDScrollView:

            MDGridLayout:
                cols: 1
                
                size_hint_y: None
                height: self.minimum_height
                padding: dp(1), dp(32)

                MDGridLayout:
                    id: gridDataUser
                    cols: 1
                    
                    size_hint_y: None
                    height: self.minimum_height

                    
                    padding: dp(16), dp(20), dp(16), dp(1)

                    #Datos
                    MDLabel:
                        markup: True
                        padding: dp(50), dp(20), dp(16), dp(32)
                        text: "[b][i]Datos[/i][/b]"

                        halign: "left"
                        font_style: "H3"
                                
                    #input datos
                    MDBoxLayout:
                        orientation: 'vertical'
                        MDGridLayout:
                            cols: 2

                            MDGridLayout:
                                cols: 4
                                padding: dp(16)

                                MDLabel:
                                    size_hint_x: 0.025

                                MDIcon:
                                    padding: dp(1), dp(1), dp(1), dp(1)
                                    icon: "account"
                                    font_size: '36sp' 
                                    size_hint_x: 0.4


                                MDLabel:
                                    size_hint_x: 0.05
                                    
                                MDTextField:
                                    id: usernameHomeLabel
                                    text: ''
                                    hint_text: "Nombre:"
                                    mode: "rectangle"
                                    disabled: True 
                                    focus: True
                                    size_hint_x: 0.4

                            MDGridLayout:
                                cols: 4
                                padding: dp(16)


                                MDLabel:
                                    size_hint_x: 0.025


                                MDIcon:
                                    padding: dp(1), dp(1), dp(1), dp(1)
                                    icon: "account-hard-hat"
                                    font_size: '36sp' 
                                    size_hint_x: 0.4


                                MDLabel:
                                    size_hint_x: 0.05
                                    
                                MDTextField:
                                    id: accessLevelHomeLabel
                                    text: ''
                                    hint_text: "Nivel de usuario:"
                                    mode: "rectangle"
                                    disabled: True 
                                    focus: True
                                    size_hint_x: 0.4

                MDGridLayout:
                    cols: 1
                    
                    size_hint_y: None
                    height: root.ids.gridDataUser.height * 12
                    
                    padding: dp(16), dp(20), dp(16), dp(16)

                    #Dinero
                    MDLabel:
                        markup: True
                        padding: dp(50), dp(100), dp(16), dp(16)
                        text: "[b][i]Cuenta[/i][/b]"

                        halign: "left"
                        font_style: "H3"
                                
                    MDBoxLayout:
                        orientation: 'vertical'
                        
                        MDGridLayout:
                            cols: 2

                            MDGridLayout:
                                cols: 4
                                padding: dp(16)

                                MDLabel:
                                    size_hint_x: 0.025

                                MDIcon:
                                    padding: dp(1), dp(1), dp(1), dp(1)
                                    icon: "account"
