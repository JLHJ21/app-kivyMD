                                    size_hint_x: 0.4

                            MDGridLayout:
                                cols: 4
                                padding: dp(16)


                                MDLabel:
                                    size_hint_x: 0.025


                                MDIcon:
                                    padding: dp(1), dp(1), dp(1), dp(1)
