                                MDLabel:
                                    size_hint_x: 0.05
                                    
                                MDTextField:
                                    text: "24$"
                                    hint_text: "Dinero en pesos:"
                                    mode: "rectangle"
                                    disabled: True 
                                    focus: True
                                    size_hint_x: 0.4

                            MDGridLayout:
                                cols: 4
                                #padding: dp(32), dp(1), dp(32), dp(16)


                                MDLabel:
                                    size_hint_x: 0.025


                                MDIcon:
                                    padding: dp(1), dp(1), dp(1), dp(1)
                                    icon: "cash-minus"
                                    font_size: '36sp' 
                                    size_hint_x: 0.4


                                MDLabel:
                                    size_hint_x: 0.05
                                    
                                MDTextField:
                                    text: "24$"
                                    hint_text: "Dinero en bolívares:"
                                    mode: "rectangle"
                                    disabled: True 
                                    focus: True
                                    size_hint_x: 0.4
                        


